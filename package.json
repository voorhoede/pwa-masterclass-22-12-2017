{
  "name": "pwa-masterclass",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node server.js",
    "predev": "npm run build",
    "dev": "nodemon --ignore 'cache/' --ignore 'src/' server.js",
    "proxy": "npm-run-all --parallel proxy:*",
    "proxy:local": "browser-sync start --proxy 'localhost:7924' --port 7925",
    "proxy:public": "ngrok http 7924",
    "prebuild": "rimraf cache/",
    "build": "run-p build:*",
    "build:static": "node scripts/copy.js",
    "build:css": "cross-env NODE_ENV=production node scripts/postcss.js",
    "build:js": "rollup --config rollup.config.js --environment NODE_ENV:production",
    "postbuild": "npm run revision",
    "revision": "node scripts/revision-hash.js",
    "postrevision": "node scripts/revision-replace.js",
    "watch": "run-p watch:*",
    "watch:serviceworker": "rollup --config rollup.config.js --watch",
    "watch:js": "rollup --config rollup.config.js --watch",
    "watch:css": "chokidar 'src/**/*.css' --command 'node scripts/postcss.js'"
  },
  "author": "De Voorhoede",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "1.2.0",
    "ngrok": "2.2.24",
    "nodemon": "1.12.4"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "cross-env": "5.1.1",
    "cssnano": "3.10.0",
    "express": "4.16.2",
    "gulp": "3.9.1",
    "gulp-filter": "5.0.1",
    "gulp-rev": "8.1.0",
    "gulp-rev-css-url": "0.1.0",
    "gulp-rev-replace": "0.4.3",
    "helmet": "3.9.0",
    "mkdirp": "0.5.1",
    "npm-run-all": "4.1.2",
    "nunjucks": "3.0.1",
    "pixrem": "4.0.1",
    "postcss": "6.0.14",
    "postcss-calc": "6.0.1",
    "postcss-color-function": "4.0.1",
    "postcss-color-rgba-fallback": "3.0.0",
    "postcss-custom-properties": "6.2.0",
    "postcss-import": "11.0.0",
    "rimraf": "2.6.2",
    "rollup": "0.51.6",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-eslint": "3.0.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-uglify": "1.0.1",
    "rollup-watch": "3.2.2",
    "shrink-ray": "0.1.3",
    "web-push": "3.2.5"
  }
}
